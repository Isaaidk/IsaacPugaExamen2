<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IsaacPugaExamen2.IPTelefonopage"
             Title="IPTelefonopage">
    <VerticalStackLayout>
        <Label 
            Text="Ingrese el numero telefonico"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Editor x:Name="TelefonoEdit"
                 Placeholder="Ingresa tu numero telefonco"
                  Text="{Binding Telefono}"
        

                 HeightRequest="100" />
        
        
        <Label Text="Ingresa tu Nombre " >
            
            
            
        </Label>
        <Editor x:Name="NombreEdit"
          Placeholder="Ingresa tu numero telefonco"
           Text="{Binding Nombre}"
                

          HeightRequest="100" />

        <Button Text="Save"
        Clicked="SaveButton_Clicked" />
        
        
    </VerticalStackLayout>
</ContentPage>